{
  "compilerOptions": {
    "target": "es5",
    "lib": [],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "typedocOptions": {
    "mode": "file",
    "out": "docs"
  },
  "include": [
    "../../../src"
  ]
}
